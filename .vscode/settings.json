{
  "emmet.includeLanguages": { "jinja-html": "html" },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "cSpell.words": [
    "codecov",
    "from",
    "leaderboard",
    "pallets",
    "pyfile",
    "pytest"
  ],
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "files.associations": { "*.j2": "html" }
}
