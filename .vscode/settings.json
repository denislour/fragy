{
  "emmet.includeLanguages": { "jinja-html": "html" },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "cSpell.words": [
    "chmod",
    "codecov",
    "endmacro",
    "from",
    "leaderboard",
    "noqa",
    "pallets",
    "pyfile",
    "pytest"
  ],
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "files.associations": {
    "*.j2": "html"
  },
  "python.linting.flake8Enabled": true
}
